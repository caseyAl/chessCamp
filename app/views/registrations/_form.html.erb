<%= simple_form_for(@registration) do |f| %>
    <%= f.error_notification %>
    <div class="row">
      <div class="form-inputs">
        <%= f.input :student_id, label_html: { class: 'active' } %>
        <%= f.input :camp_id, label_html: { class: 'active' } %>
      </div>
    </div>
    <div class="row">
      <div class="form-actions col s8">
        <%= f.button :submit %>
      </div>
    </div>
<% end %>